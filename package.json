{
  "name": "torrent-bot",
  "version": "1.0.0",
  "description": "Torrent bot for downloading and searching torrents",
  "main": "index.js",
  "scripts": {
    "dev": "DEBUG=app nodemon --exec babel-node index.js",
    "build-web": "cd web && npm install && npm run build",
    "build-dist": "rm -rf ../dist-app/* && ./node_modules/.bin/babel . --out-dir ../dist-app --ignore node_modules,web,config.dev.js,proxy.js && cp -r web ../dist-app/web && cp package.json .gitignore ../dist-app",
    "serve": "node dist/index.js",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "torrent",
    "bot",
    "download",
    "search",
    "seed"
  ],
  "author": "pathetic_geek",
  "license": "ISC",
  "nodemonConfig": {
    "ignore": [
      ".next",
      "node_modules",
      "web"
    ]
  },
  "prettier": {
    "printWidth": 140,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "diskusage": "^1.1.3",
    "emoji-strip": "^1.0.1",
    "express": "^4.17.1",
    "googleapis": "39",
    "http-proxy": "^1.18.0",
    "lodash": "^4.17.21",
    "net": "^1.0.2",
    "node-telegram-bot-api": "^0.40.0",
    "nodemon": "^2.0.2",
    "prop-types": "^15.7.2",
    "puppeteer": "^2.0.0",
    "readline": "^1.3.0",
    "serve-index": "^1.9.1",
    "tall": "^5.0.0",
    "webtorrent": "^0.107.17",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1",
    "zip-a-folder": "^0.0.12"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0"
  }
}
